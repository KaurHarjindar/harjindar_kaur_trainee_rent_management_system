<odoo>

    <template id="report_tenant_view">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <table border="1">
                            <tr>
                                <th>Tenant Name</th>
                                <th>Dob</th>
                                <th>Gender</th>
                                <th>Phone</th>
                                <th>Email</th>
                                <th>Religion</th>
                                <th>Marital status</th>
                                <th>Permanant Address</th>
                                <th>Occupation</th>
                                <th>Father's Name</th>
                                <th>Mobile Number</th>
                            </tr>
                            <tr>
                                <td><h2 t-field="doc.name"/></td>
                                <td><h2 t-field="doc.dob"/></td>
                                <td><h2 t-field="doc.gender"/></td>
                                <td><h2 t-field="doc.phone"/></td>
                                <td><h2 t-field="doc.email"/></td>
                                <td><h2 t-field="doc.Occupation"/></td>                               
                                <td><h2 t-field="doc.Marital_status"/></td>
                                <td><h2 t-field="doc.Religion"/></td>
                                <td><h2 t-field="doc.Home_address"/></td>
                                <td><h2 t-field="doc.Father_name"/></td>
                                <td><h2 t-field="doc.Fphone"/></td>
                            </tr>
                        </table>
                       
                        <!-- <h3>Attendees:</h3>
                        <ul>
                            <t t-foreach="doc.attendee_ids" t-as="attendee">
                                <li><span t-field="attendee.name"/></li>
                            </t>
                        </ul> -->
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>