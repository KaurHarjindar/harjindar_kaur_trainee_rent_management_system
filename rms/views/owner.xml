    <record model="ir.ui.view" id="PropertyForm">
        <field name="name">training  property</field>
        <field name="model">training.property</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="ptype" />
                        <field name="address" />
                        <field name="city" />
                        <field name="state" />
                        <field name="location" />
                        <field name="ownername" />
                        <field name="policies" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <record model="ir.ui.view" id="propertyTree">
        <field name="name">training property</field>
        <field name="model">training.property</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="ptype" />
                <field name="address" />
                <field name="city" />
                <field name="state" />
                <field name="location" />
                <field name="ownername" />
                <field name="policies" />
            </tree>
        </field>
    </record>

    <record model="ir.ui.view" id="ownerTree">
        <field name="name">training owner</field>
        <field name="model">training.owner</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="lastname" />
                <field name="number" />
                <field name="email" />
                <field name="address" />
                <field name="city" />
                <field name="state" />
            </tree>
        </field>
    </record>

    <record model="ir.ui.view" id="ownerForm">
        <field name="name">training  owner</field>
        <field name="model">training.owner</field>
        <field name="arch" type="xml">
            <form>
               <!--  <header clickable="true">
                    <button name="button_start" string="Started" states="Running" type="object"  class="oe_highlight"/>
                    <button name="button_running" string="Running mode" states="Start,Complete" type="object"    class="oe_highlight"/>
                    <button name="button_Stop" string="Completed" type="object" states="Start,Running"  class="oe_highlight"/>
                    <field  name="state" widget="statusbar"  />
                </header> -->
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="lastname" />
                        <field name="number" />
                        <field name="email" />
                        <field name="address" />
                        <field name="city" />
                        <field name="state" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="owner_search_view">
            <field name="name">owner_search</field>
            <field name="model">training.owner</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                </search>
            </field>
    </record>

    

    <record id="owner_action" model="ir.actions.act_window">
        <field name="name">training owner</field>
        <field name="res_model">training.owner</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="property_action" model="ir.actions.act_window">
        <field name="name">training property</field>
        <field name="res_model">training.property</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="training_rooms" model="ir.actions.act_window">
        <field name="name">training rooms</field>
        <field name="res_model">training.rooms</field>
        <field name="view_mode">tree,form,kanban</field>
    </record>

     <record id="training_tenantroomallocate" model="ir.actions.act_window">
        <field name="name">training tenantRoomAllocate</field>
        <field name="res_model">training.tenantroomallocate</field>
        <field name="view_mode">tree,form,kanban,calendar,graph</field>
    </record>

    <menuitem id="tenants_owner_root" name="Owner" />
    <menuitem id="owner_regi" name="Registration" parent="tenants_owner_root" action="owner_action"/>
    <menuitem id="propertyRegi" name="AddProperty" parent="tenants_owner_root" action="property_action" />
    <menuitem id="tenants_rooms" name="Add Room" parent="tenants_owner_root" action="training_rooms" />
    <menuitem id="tenants_room_allocate" name="Allocate Room" parent="tenants_owner_root" action="training_tenantroomallocate" />
</odoo>