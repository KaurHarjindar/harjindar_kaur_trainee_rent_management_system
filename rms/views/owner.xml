<odoo>

     <record model="ir.ui.view" id="alloteview">
        <field name="name">training.calendar</field>
        <field name="model">training.tenantroomallocate</field>
        <field name="arch" type="xml">
            <calendar string="Ideas" date_start="Joining_date"  color="Joining_date">
                <field name="roomnumber"/>
                <field name="rent" />
            </calendar>
        </field>
    </record> 

    <record model="ir.ui.view" id="alloteviewGraph">
        <field name="name">training.Graph</field>
        <field name="model">training.tenantroomallocate</field>
        <field name="arch" type="xml">
            <graph>
                <field name="roomnumber"/>
                <field name="rent" type="measure" />
            </graph>
        </field>
    </record> 

    <record model="ir.ui.view" id="kanban_view">
        <field name="name">training.kanban</field>
        <field name="model">training.tenantroomallocate</field>
        <field name="arch" type="xml">
            <kanban default_group_by="pname">
                <field name="roomnumber" />
                <field name="tenantname" />
                <field name="rent" />
                <field name="status" />
                <templates>
                   <t t-name="kanban-box">
                        <div t-attf-class="oe_kanban_global_click">
                            <div class="oe_kanban_details">
                                <strong class="o_kanban_record_title">
                                <b>Roomnumber:</b>
                                <field name="roomnumber"/>
                                <field name="pname" />
                                </strong>
                                <div t-if="record.tenantname.value">
                                    <b>Tenant detail:</b>
                                    <t t-esc="record.tenantname.value"/>
                                </div>
                                <div t-if="record.rent.value">
                                    <b>Rent:</b>
                                    <t t-esc="record.rent.value"/>
                                </div>
                                <div t-if="record.status.value">
                                    <b>Status</b>
                                    <t t-esc="record.status.value" />
                                </div>

                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

     <record model="ir.ui.view" id="tenantselection">
        <field name="name">training tenantroomallocate</field>
        <field name="model">training.tenantroomallocate</field>
        <field name="arch" type="xml">
            <tree string="tree view" >
                <field name="pname" />
                <field name="roomnumber" />
                <field name="tenantname" />
                <field name="rent" />
                <field name="status" />
                <field name="Joining_date" />
            </tree>
        </field>
    </record>

    <record id="room_kanban" model="ir.ui.view">
        <field name="name">training rooms kanban</field>
        <field name="model">training.rooms</field>
        <field name="arch" type="xml">
            <kanban default_group_by="status">
                <field name="propertyname" />
                <field name="block" />
                <field name="floor" />
                <field name="roomnumber" />
                <field name="ownerName" />
                <templates>
                    <t t-name="kanban-box">
                        <div t-attf-class="oe_kanban_global_click">
                            <div class="oe_kanban_details">
                                <strong class="o_kanban_record_title">
                                    <b>propertyname</b>
                                    <field name="propertyname" />
                                    <field name="roomnumber" />
                                </strong>
                                <div t-if="record.block">
                                    <b>block</b>
                                    <t t-esc="record.block.value"/>
                                </div>
                                <div t-if="record.floor">
                                    <b>Floor</b>
                                    <t t-esc="record.floor.value"/>
                                </div>
                                <div t-if="record.ownername">
                                    <b>OwnerName</b>
                                    <t t-esc="record.ownerName.value"/>
                                </div>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

