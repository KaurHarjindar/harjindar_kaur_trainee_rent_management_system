<odoo>
	<record id="room_tree" model="ir.ui.view">
        <field name="name">training.room</field>
        <field name="model">training.room</field>
        <field name="arch" type="xml">
            <tree string="room tree"  >
                <field name="hostelId" />
                <field name="roomNumber" />
            </tree>
        </field>
    </record>

     <record id="training_room_regi" model="ir.actions.act_window">
        <field name="name">training room</field>
        <field name="res_model">training.room</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="tenants_room" name="Room" parent="tenants_owner_root"  />
    <menuitem id="tenants_room1" name="RoomRegi" parent="tenants_room" action="training_room_regi"  />
</odoo>